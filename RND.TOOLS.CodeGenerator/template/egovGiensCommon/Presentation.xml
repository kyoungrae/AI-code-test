    <source>
        <![CDATA[
<style type="text/css">
</style>
<div class="html#Service#">
	<!-- main section -->
	<section class="panel mainPanel dl-col-all-12">
		<!-- header -->
		<header class="panel-heading menu-panel-heading">
			<h2 class="panel-title menu-panel-title">MENUTITLE</h2>
			<div class="panel-actions"></div>
		</header>
		<!-- //header -->
		<!-- body -->
		<div class="panel-body">
			<div class="col-md-12" data-plugin-scrollable >
				<!-- search area -->
				<div class="dl-search-section">
					<div class="search-container dl-col-all-12">
						<!-- ////////// search option start ////////// -->
						<div class="form-horizontal dl-period-more-container dl-float-left">
							<div class="search-form-input dl-period-area">
								<div class="dl-period-input-area input-daterange input-group daterange-border-left dl-float-end" data-plugin-datepicker>
									<input type="text" class="form-control dl-float-left" id="date_from" name="date_from" placeholder="formUtil.Message.SetStartDate" autocomplete="off"/>
									<span class="input-group-addon dl-float-left">~</span>
									<input type="text" class="form-control dl-float-left" id="date_to" name="date_to" placeholder="formUtil.Message.SetEndDate" autocomplete="off"/>
								</div>
								<div id="periodFilter" class="dl-period-filter-area"></div>
							</div>
						</div>
						<div class="dl-search-more-container dl-float-left">
							<div class="input-group form-horizontal search-form-input dl-search-input-area">
								<input id="#service#_Desc" type="text" class="form-control" placeholder="formUtil.Message.InputSearchWord" autocomplete="off"/>
							</div>
						</div>
						<div class="dl-btn2-container dl-float-left">
							<div class="more-btn-area">
								<button type="button" id="more_btn">+ <span class="disp-label">BTN_MORE</span></button>
							</div>
						</div>
						<div class="dl-btn2-container dl-float-left">
							<div class="search-btn-area">
								<button id="resetBtn" class="default-click"><i class="fa fa-repeat"></i></button>
								<button id="seaBtn" class="btn btn-sm btn-primary search-click"><i class="fa fa-search"></i></button>
							</div>
						</div>
						<div class="dl-more-container dl-float-end">
							<!-- 더보기에 들어갈 항목들 넣는 영역  -->
							<div class="dl-more-search-area dl-float-left">
								<div class="select-group dl-select-area">
									<select class="form-control" id="#service#_Select"></select>
								</div>
							</div>
							<div class="dl-more-search-area dl-float-left">
								<div class="input-group form-horizontal search-form-input dl-search-input-area">
									<input id="#service#_Desc1" type="text" class="form-control" placeholder="formUtil.Message.InputSearchWord" autocomplete="off"/>
								</div>
							</div>
						</div>
						<!-- ////////// search option end ////////// -->
					</div>
					<div class="hr dl-col-all-12"></div>
				</div>
				<!-- grid list count, search button-->
				<div class="dl-info-area">
					<div class="dl-showNo dl-float-left">
						<select id="showNoGrid"></select>
					</div>
					<div class="totalLabel dl-float-left">
						<span class="disp-label">LIST_TOTAL_COUNT_TOT</span>  <b id="#service#_totalCount">0</b><span class="disp-label">LIST_TOTAL_COUNT_CNT</span>
					</div>
				</div>
				<!-- //grid list count, search button-->

				<!-- grid & paging -->
				<div class="grid-area">
					<div class="grid-container mobile-grid-form">
						<div id="#service#_Grid"></div>
					</div>
					<div id="#service#_Paging" class="crud_Paging"></div>
				</div>
			</div>
		</div> <!-- // panel-body -->

		<!-- footer -->
		<footer class="panel-footer text-right dl-bottom-btn-container">
			<button class="btn btn-sm btn-success popup-View_Popup-click"><i class="el el-view-mode"></i><span class="disp-label">BTN_DETAIL</span></button>
			<button class="btn btn-sm btn-success popup-Add_Popup-click"><i class="fa fa-plus"></i> <span class="disp-label">BTN_INSERT</span></button>
			<button class="btn btn-sm btn-success popup-Modify_Popup-click"><i class="el el-edit"></i><span class="disp-label">BTN_MODIFY</span></button>
			<button class="btn btn-sm btn-success popup-Drop_Popup-click"><i class="fa fa-minus"></i><span class="disp-label">BTN_REMOVE</span></button>
			<button class="btn btn-sm btn-success excel-click"><i class="fa fa-download"></i><span class="disp-label">BTN_EXCEL</span></button>
		</footer>
		<!-- //footer -->

	</section>
	<!-- //main section -->


	<!-- ========================= popup ========================= -->
	<!-- view popup -->
	<div id="View_Popup" class="modal-block modal-block-primary mfp-hide">
		<section class="panel">
			<header class="panel-heading menu-panel-heading">
			<h2 class="panel-title menu-panel-title"><i class="fa fa-caret-right"></i><span class="menu-popup-title">MENUPOPUPTITLE</span><span class="disp-label">DETAIL</span></h2>
			</header>

			<div class="panel-body">
				<div class="dl-col-all-12 dl-box-container">
#begin#
					<div class ="dl-col-all-12">
						<div class="dl-col-all-3 dl-box-item-col1">
							<label class="dl-box-item-label"><span class="disp-label">#field_desc#</span></label>
						</div>
						<div class="dl-col-all-9 dl-box-item-col2">
							<label class="dl-box-item-label" id="view_#field_name#"></label>
						</div>
					</div>#end#
				</div>
			</div>
			<footer class="panel-footer">
				<div class="row">
					<div class="dl-col-all-12 text-right">
						<button class="btn btn-default modal-dismiss close-popup-click"><i class="fa fa-minus"></i><span class="disp-label">BTN_CLOSE</span></button>
					</div>
				</div>
			</footer>
		</section>
	</div>
	<!-- //view popup -->

	<!-- add popup -->
	<div id="Add_Popup" class="modal-block modal-block-primary mfp-hide">
		<section class="panel">
			<header class="panel-heading menu-panel-heading">
			<h2 class="panel-title menu-panel-title"><i class="fa fa-caret-right"></i><span class="menu-popup-title">MENUPOPUPTITLE</span><span class="disp-label">REGISTRATION</span></h2>
			</header>

			<div class="panel-body">
				<form id="Add_PopupForm" class="form-horizontal mb-lg" novalidate="novalidate">
#begin#
					<div class="form-group mt-lg">
						<label class="col-sm-3 control-label">
							<span class="disp-label">#field_desc#</span>
						</label>
						<div class="col-sm-9">
							<input class="form-control" id="add_#field_name#" type="text" name="#field_name#" dataField="#field_name#" data-plugin-maxlength maxlength="#length#" placeholder="ex)f2032ed79a33" autocomplete="off"/>
						</div>
					</div>#end#
				</form>
			</div>

			<footer class="panel-footer">
				<div class="row">
					<div class="col-md-12 text-right">
						<button class="btn btn-primary modal-confirm save-Add_Popup-click"><i class="fa fa-plus"></i><span class="disp-label">BTN_SAVE</span></button>
						<button class="btn btn-default modal-dismiss close-Add_Popup-click"><i class="fa fa-remove"></i><span class="disp-label">BTN_CANCEL</span></button>
					</div>
				</div>
			</footer>
		</section>
	</div>
	<!-- //add popup -->

	<!-- modify popup -->
	<div id="Modify_Popup" class="modal-block modal-block-primary mfp-hide">
		<section class="panel">
			<header class="panel-heading menu-panel-heading">
			<h2 class="panel-title menu-panel-title"><i class="fa fa-caret-right"></i><span class="menu-popup-title">MENUPOPUPTITLE</span><span class="disp-label">MODIFY</span></h2>
			</header>

			<div class="panel-body">
				<form id="Modify_PopupForm" class="form-horizontal mb-lg" novalidate="novalidate">
#begin#
					<div class="form-group mt-lg">
						<label class="col-sm-3 control-label">
							<span class="disp-label">#field_desc#</span>
						</label>
						<div class="col-sm-9">
							<input class="form-control" id="modify_#field_name#" type="text" name="#field_name#" dataField="#field_name#" data-plugin-maxlength maxlength="#length#" placeholder="ex)f2032ed79a33" autocomplete="off"/>
						</div>
					</div>#end#
				</form>
			</div>

			<footer class="panel-footer">
				<div class="row">
					<div class="col-md-12 text-right">
						<button class="btn btn-primary modal-confirm save-Modify_Popup-click"><i class="el el-edit"></i><span class="disp-label">BTN_SAVE</span></button>
						<button class="btn btn-default modal-dismiss close-Modify_Popup-click"><i class="fa fa-remove"></i><span class="disp-label">BTN_CANCEL</span></button>
					</div>
				</div>
			</footer>
		</section>
	</div>
	<!-- //modify popup -->

	<!-- drop popup -->
	<div id="Drop_Popup" class="modal-block modal-block-primary mfp-hide">
		<section class="panel">
			<header class="panel-heading menu-panel-heading">
				<h2 class="panel-title menu-panel-title"><i class="fa fa-caret-right"></i><span class="menu-popup-title">MENUPOPUPTITLE</span><span class="disp-label">ALERT_DELETE</span></h2>
			</header>

			<div class="panel-body">
				<div class="dl-col-all-12 dl-box-container">
#begin#
					<div class ="dl-col-all-12">
						<div class="dl-col-all-3 dl-box-item-col1">
							<label class="dl-box-item-label"><span class="disp-label">#field_desc#</span></label>
						</div>
						<div class="dl-col-all-9 dl-box-item-col2">
							<label class="dl-box-item-label" id="drop_#field_name#"></label>
						</div>
					</div>#end#
				</div>
				<form id="Drop_PopupForm" class="form-horizontal" novalidate="novalidate">
						<div class="form-group mt-lg text-center">
							<label class="control-label confirm-label disp-label">_MESSAGE_CONFIRM_ALRET</label>
#begin#
							<input class="form-control" id="drop_keyword_#key_fields#" type="hidden" name="#key_fields#" dataField="#key_fields#" required/>	#end#
						</div>
					</form>

			</div>

			<footer class="panel-footer">
				<div class="row">
					<div class="col-md-12 text-right">
						<button class="btn btn-primary modal-confirm save-Drop_Popup-click"><i class="fa fa-trash"></i><span class="disp-label">BTN_REMOVE</span></button>
						<button class="btn btn-default modal-dismiss close-Drop_Popup-click"><i class="fa fa-remove"></i><span class="disp-label">BTN_CANCEL</span></button>
					</div>
				</div>
			</footer>
		</section>
	</div>
	<!-- //========================= popup ========================= -->

</div><!-- // page -->

<script type="text/javascript">
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// variables
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

var PRGMID = "html#Service#";	//page id

//filter variable
var periodFilter=null;
var periodTotalCheck=false;

//날짜 값 저장
var now_from=$("#date_from").val();
var now_to=$("#date_to").val();

//grid variable
var __ROWCOUNT_PER_PAGE__ = 10;  //grid rowcount
var #service#_Grid = null; //grid instance
var #service#_Paging = null; //paging instance

var updateSupportCodeLock = false; //update lock

//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// init functions
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

/**
 * main onload
 */
WatDocument.onload = function() {

	WatTop.documentDivResize( #service#_resize );

	initEvts();
	initGrid();
	initSelect();
	initFilter();
	initDefaultValue();
	refreshCtrlBts();

	formUtil.Common.setTimeout(0.5,function(){
		window.onresize();
		search#Service#();
	});

};

function initFilter(){
	periodFilter=new formUtil.Custom.PeriodFilter("periodFilter","date_from","date_to");
	periodFilter.setFilter();
	periodFilter.setFilterType("basic"); // [basic | week | month | year | quarter | half-year | quarter-year]
	periodFilter.init();
	periodFilter.setEventOnClick(function(){
		periodTotalCheck=periodFilter.totalPeriodChk();
	});
}
/**
 * datepicker&input&select init setting
 */
function initDefaultValue(){
	formUtil.Common.setControlValue("date_from", formUtil.Util.Date.getFormatDate.yyyy_mm_dd(formUtil.Util.Date.getThisMonthFirstday()));
	formUtil.Common.setControlValue("date_to", formUtil.Util.Date.getFormatDate.yyyy_mm_dd(formUtil.Util.Date.getToday()));
	formUtil.Common.setControlValue("#service#_Desc","");
	//more 영역 초기화
	formUtil.Common.setControlValue("#service#_Desc1","");
	$('##service#_Select option:eq(0)').prop('selected', true);
}

/**
 * row count & crud_Select select init
 */
function initSelect(){
	 //row카운트 추가 부분
	var rowCountSelect=new formUtil.Custom.CodeSelect("showNoGrid","GRID_ROW_COUNT");
	rowCountSelect.setEventOnChange(function(val,text){
		if(0==val)return;
		window.localStorage.setItem(PRGMID+".rowCountSelect",val);
		search#Service#();
	});
	rowCountSelect.visibleTitle(true);
	rowCountSelect.init();
	if(window.localStorage.getItem(PRGMID+".rowCountSelect")){
		rowCountSelect.selectValue(window.localStorage.getItem(PRGMID+".rowCountSelect"));
	}

	//more에 있는 select 셋팅
	var #service#Select=new formUtil.Custom.CodeSelect("#service#_Select","TEST_CODE");
	#service#Select.setEventOnChange(function(val,text){
		if(0==val)return;
	});
	#service#Select.visibleTitle(true);
	#service#Select.init();
}

/**
 * init event
 */
function initEvts() {

	// excel button click event
	formUtil.Common.setEvt(PRGMID, "excel-click", "click", function(evt) {
		#service#_Grid.toExcel();
	});

	// search button click event
	formUtil.Common.setEvt(PRGMID,"search-click","click",function(evt){
		initSearchOptions();
		search#Service#();
	});

	// reset button click event
	formUtil.Common.setEvt(PRGMID, "default-click", "click", function(evt) {
		initDefaultOptions();
		search#Service#();
	});

	// view button click event
	formUtil.Common.setEvt(PRGMID,"popup-View_Popup-click","click",function(evt){
		var rowId = #service#_Grid.getSelectedRowId();
		popupView#Service#(rowId);
	});

	// add button click event
	formUtil.Common.setEvt(PRGMID,"popup-Add_Popup-click","click",function(evt){
		popupAdd#Service#();
	});

	// modify button click event
	formUtil.Common.setEvt(PRGMID,"popup-Modify_Popup-click","click",function(evt){
		var rowId = #service#_Grid.getSelectedRowId();
		popupModify#Service#(rowId);
	});

	// drop button click event
	formUtil.Common.setEvt(PRGMID,"popup-Drop_Popup-click","click",function(evt){
		var rowId = #service#_Grid.getSelectedRowId();
		popupDrop#Service#(rowId);
	});

	// add popup - save button click event
	formUtil.Common.setEvt(PRGMID,"save-Add_Popup-click","click",function(evt){
		if (!isValidAdd#Service#())
			return;

		updateSupportCodeLock = true;
		var content = formUtil.Common.getFormValues("Add_PopupForm");
		updateSupportCodeLock = false;
		add#Service#(content);

		formUtil.Common.closePopup();
	});

	// modify popup - save button click event
	formUtil.Common.setEvt(PRGMID,"save-Modify_Popup-click","click",function(evt){
		if (!isValidModify#Service#())
			return;

		updateSupportCodeLock = true;
		var content = formUtil.Common.getFormValues("Modify_PopupForm");
		updateSupportCodeLock = false;
		modify#Service#(content);

		formUtil.Common.closePopup();
	});

	// drop popup - save button click event
	formUtil.Common.setEvt(PRGMID,"save-Drop_Popup-click","click",function(evt){
		if (!isValidDrop#Service#())
			return;

		updateSupportCodeLock = true;
		var content = formUtil.Common.getFormValues("Drop_PopupForm");
		updateSupportCodeLock = false;
		drop#Service#(content);

		formUtil.Common.closePopup();
	});

	// popup close button click event - view close
	formUtil.Common.setEvt(PRGMID,"close-popup-click","click",function(evt){
		formUtil.Common.closePopup();
	});

	// popup close button click event - add cancel
	formUtil.Common.setEvt(PRGMID,"close-Add_Popup-click","click",function(evt){
		formUtil.Common.closePopup();
	});

	// popup close button click event - modify cancel
	formUtil.Common.setEvt(PRGMID,"close-Modify_Popup-click","click",function(evt){
		formUtil.Common.closePopup();
	});

	// popup close button click event - drop cancel
	formUtil.Common.setEvt(PRGMID,"close-Drop_Popup-click","click",function(evt){
		formUtil.Common.closePopup();
	});
}

/**
 * main resize event handler
 */
function #service#_resize(wid,hei) {
	var serHei = hei;
	if (#service#_Grid) {
		$("##service#_Grid").css("height","auto");
		$("##service#_Grid objbox").css("height","auto");
	}
}

/**
 * grid & paging initialize
 */
function initGrid() {
	try {

		//grid
		#service#_Grid = new formUtil.Component.Grid("#service#_Grid");
		#service#_Grid.setHeader([
			//{"KEY":"NO",		"NAME":"No.",										"WIDTH":"50",		"ALIGN" : "center",	"SORT" : "int", "TYPE" : "ro"},#begin#
			{"KEY" : "#field_name#",	"NAME" : formUtil.Label.Array["#field_desc#"],	"WIDTH" : "*",	"ALIGN" : "center",	"SORT" : "str",	"TYPE" : "ro"}#conjunction##end#,
			//{"KEY" : "sys_cr_user_nm",	"NAME" : formUtil.Label.Array["#scope#.#service#.sys_cr_user_nm"],		"WIDTH" : "*",	"ALIGN" : "center",	"SORT" : "str",	"TYPE" : "ro"},
			//{"KEY" : "sys_ud_user_nm",	"NAME" : formUtil.Label.Array["#scope#.#service#.sys_ud_user_nm"],		"WIDTH" : "*",	"ALIGN" : "center",	"SORT" : "str",	"TYPE" : "ro"}
			]);
		#service#_Grid.setEventOnRowSelect(function(rid,idx){
			refreshCtrlBts();
		});
		#service#_Grid.setEventOnRowDoubleClicked(function(id,ind){
			popupView#Service#(id);
		});
		#service#_Grid.enableAutoHeight(true);
		#service#_Grid.init({showNoDataRow:true});

		//paging
		#service#_Paging = new formUtil.Component.Page("#service#_Paging");
		#service#_Paging.setRowCount(__ROWCOUNT_PER_PAGE__);
		#service#_Paging.init();

	} catch (e) {
		formUtil.Script.exception(PRGMID + " initGrid", e);
	}
}

//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// popup functions
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

/**
* 상세 팝업
*/
function popupView#Service#(rowId) {
	// 상세 팝업 실행
	formUtil.Common.popup("View_Popup");
	$("#View_Popup").focus();

#begin#
	formUtil.Common.setControlValue("view_#field_name#", #service#_Grid.getValueByColId(rowId, "#field_name#"));#end#
}

/**
* 등록 팝업
*/
function popupAdd#Service#() {
	// 등록 팝업 실행
	formUtil.Common.popup("Add_Popup");
	$("#Add_PopupForm").focus();

	//input data reset
	$("#Add_PopupForm")[0].reset();
	formUtil.Common.initRequiredValidation("Add_PopupForm");
}

/**
* 수정 팝업
*/
function popupModify#Service#(rowId) {
	// 수정 팝업 실행
	formUtil.Common.popup("Modify_Popup");
	$("#Modify_PopupForm").focus();

	formUtil.Common.initRequiredValidation("Modify_PopupForm");

#begin#
	formUtil.Common.setControlValue("modify_#field_name#", #service#_Grid.getValueByColId(rowId, "#field_name#"));#end#
}

/**
* 삭제 팝업
*/
function popupDrop#Service#(rowId) {
	// 삭제 팝업 실행
	formUtil.Common.popup("Drop_Popup");
	$("#Drop_Popup").focus();

#begin#
	formUtil.Common.setControlValue("drop_#field_name#", #service#_Grid.getValueByColId(rowId, "#field_name#"));#end#
#begin#
	formUtil.Common.setControlValue("drop_keyword_#key_fields#", #service#_Grid.getValueByColId(rowId, "#key_fields#"));#end#
}

//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// validation functions
//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

/**
 * popupAdd#Service# validation
 */
function isValidAdd#Service#(){
	// 필수 입력값 체크
	if (!formUtil.Common.requiredValidation("Add_PopupForm"))
		return false;
	return true;
}

/**
 * popupModify#Service# validation
 */
function isValidModify#Service#(){
	// 필수 입력값 체크
	if (!formUtil.Common.requiredValidation("Modify_PopupForm"))
		return false;
	return true;
}

/**
 * popupDrop#Service# validation
 */
function isValidDrop#Service#(){
	// 필수 입력값 체크
	if (!formUtil.Common.requiredValidation("Drop_PopupForm"))
		return false;
	return true;
}

//-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
// request functions
