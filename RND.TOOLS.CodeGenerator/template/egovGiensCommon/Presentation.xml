    <reserved>
        <sourcetype>html</sourcetype>
        <conjunction>,</conjunction>
        <filename>#Service#</filename>
        <ext>jsp</ext>
        <path>./work/genSrc/kr/co/giens/WebContents/www/#scope#/</path>
    </reserved>
    <definition>
        <version>1.0</version>
